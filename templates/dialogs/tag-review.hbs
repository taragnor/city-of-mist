<div class="purpose"> {{localize "CityOfMist.dialog.tagReview.purpose"}} </div>
<div class="move-name">
	<span class="label">
		{{localize "CityOfMist.terms.move"}}:
	</span>
	{{move.displayedName}}
</div>
{{#each tagAndStatusList as |item|}}
<div class=".approval-status" >{{item.review}} </div>
<div class="tag" data-item-id="{{item.item.id}}" data-owner-id="{{item.item.parent.id}}">
	<div>
		{{> "systems/city-of-mist/templates/parts/tag-or-status-name.hbs" item=item.item sheetowner=null locked=true}}
	</div>
	<div class= "review-icons">
		<a class="item-control approved" title="{{localize 'CityOfMist.dialog.tagReview.approve'}}"><i class="fas fa-check"></i></a>
		<a class="item-control request-clarification" title="{{localize 'CityOfMist.dialog.tagReview.requestClarification'}}"><i class="fas fa-question"></i></a>
		<a class="item-control rejected" title="{{localize 'CityOfMist.dialog.tagReview.reject'}}"><i class="fas fa-cancel"></i></a>
	</div>
</div>
{{/each}}
